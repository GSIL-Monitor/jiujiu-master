<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jiu.mapper.UserMapper" >
    <resultMap id="BaseResultMap" type="com.jiu.model.User" >
        <id column="user_id" property="userId" jdbcType="INTEGER" />
        <result column="user_name" property="userName" jdbcType="VARCHAR" />
        <result column="pass_word" property="passWord" jdbcType="VARCHAR" />
        <result column="phone" property="phone" jdbcType="VARCHAR" />
    </resultMap>
    <sql id="Base_Column_List" >
        user_id, user_name, pass_word, phone
    </sql>
    <insert id="addUser" parameterType="com.jiu.model.User">
      insert into t_user (user_name,pass_word,phone)
      VALUES (#{userName},#{passWord},#{phone})
    </insert>
</mapper>
